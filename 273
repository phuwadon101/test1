        }

        private void button1_Click(object sender, EventArgs e)
        {
            double result, x,y ;
            x = double.Parse(textBox1.Text);
            y = double.Parse(textBox2.Text);
            result = Math.Pow(x,y);
            textBox3.Text = result.ToString();
        }

